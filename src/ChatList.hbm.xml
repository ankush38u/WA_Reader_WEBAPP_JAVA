<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.electricdroid.beans">
	<class name="ChatList" table="chat_list">
		<id name="_id" column="_id" >
			<generator class="increment"></generator>
		</id>
		<property name="key_remote_jid" type="string"></property>
		<property name="subject" type="string"></property>
		<property name="last_read_message_table_id"></property>
		<property name="sort_timestamp" ></property>
		<property name="gen"></property>
		<property name="my_messages" ></property>
		<!-- <property name="last_read_receipt_sent_message_table_id "></property> -->
		</class>
</hibernate-mapping>